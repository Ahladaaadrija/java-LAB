import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n,i,f=0,s=1,t;
        System.out.println("Enter the number");
        n=sc.nextInt();
        System.out.println(f);
        System.out.println(s);
        for(i=2;i<=n;i++){
            t=f+s;
            System.out.println(t);
            f=s;
            s=t;
            
        }
    }}
